{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schema.yext.com/config/example/versioned-resource",
  "type": "object",
  "title": "Versioned Resource",
  "additionalProperties": false,
  "$version": "3.1",
  "properties": {
    "$id": {
      "type": "string"
    },
    "$schema": {
      "const": "https://schema.yext.com/config/example/versioned-resource/v3"
    },
    "previouslyStringNowIntegerValue": {
      "type": "integer"
    },
    "previouslyRequiredNowOptionalValue": {
      "type": "integer"
    },
    "previouslySingleNowListValue": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    }
  },
  "required": [
    "$id",
    "$schema",
    "previouslyStringNowIntegerValue"
  ]
}